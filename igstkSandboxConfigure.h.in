/*=========================================================================
Configure file for IGSTKSandbox
=========================================================================*/
#ifndef __igstkSandboxConfigure_h
#define __igstkSandboxConfigure_h

/* no option to build shared libs yet */
#if 0  /* @IGSTKSandbox_BUILD_SHARED_LIBS_CONFIG@ */
# define @PROJECT_NAME@_SHARED
#endif

#if defined(_MSC_VER) && defined(@PROJECT_NAME@_SHARED)
# pragma warning ( disable : 4275 )
#endif

#if defined(_WIN32) && defined(@PROJECT_NAME@_SHARED)
# if defined(@PROJECT_NAME@_EXPORTS)
#  define @PROJECT_NAME@_EXPORT __declspec( dllexport ) 
# else
#  define @PROJECT_NAME@_EXPORT __declspec( dllimport ) 
# endif
#else
# define @PROJECT_NAME@_EXPORT
#endif

/* define ITK version */
#cmakedefine ITK_PRE4_VERSION

/* options defined by cmake */
#cmakedefine IGSTK_USE_ATAMAI
#cmakedefine IGSTKSandbox_USE_FLTK
#cmakedefine IGSTKSandbox_USE_Qt
#cmakedefine IGSTKSandbox_USE_MicronTracker
#cmakedefine IGSTKSandbox_USE_Ascension3DGTracker
#cmakedefine Ascension3DG_MedSafe
#cmakedefine IGSTKSandbox_SEND_TRANSFORMS_TO_SOCKETS 
#cmakedefine IGSTKSandbox_USE_vtkKWImage
#cmakedefine IGSTKSandbox_USE_OpenIGTLink
#cmakedefine IGSTK_USE_SceneGraphVisualization
#cmakedefine IGSTKSandbox_USE_OpenCV

#cmakedefine IGSTK_DATA_ROOT "@IGSTK_DATA_ROOT@"

#cmakedefine IGSTKSandbox_DATA_ROOT "@IGSTKSandbox_DATA_ROOT@"

#cmakedefine IGSTKSandbox_BINARY_DIR "@IGSTKSandbox_BINARY_DIR@"

#endif // __igstkSandboxConfigure_h
